{
	"trailingComma": "all",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES",
		"^@/app/(.*)$",
		"^@/components/(.*)$",
		"^@/services/(.*)$",
		"^@/usecases/(.*)$",
		"^@/helpers/(.*)$",
		"^@/data/(.*)$",
		"^@/providers/(.*)$",
		"^@/assets/(.*)$",
		"^@/interfaces/(.*)$",
		"^@/hooks/(.*)$",
		"^@/utils/(.*)$",
		"^@/api/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
